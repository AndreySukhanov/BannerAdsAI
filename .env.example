# Frontend Environment Variables

# API Base URL (обычно для продакшена это ваш домен)
VITE_API_BASE_URL=http://localhost:3001

# Режим разработки (development/production)
VITE_NODE_ENV=development

# Включить/отключить отладочную информацию
VITE_DEBUG=true

# URL для интеграции виджетов
VITE_WIDGET_BASE_URL=http://localhost:4173